<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="360.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.CreateAccountController">
   <children>
      <Label layoutX="55.0" layoutY="14.0" prefHeight="24.0" prefWidth="139.0" text="Create Account">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="51.0" prefHeight="19.0" prefWidth="197.0" text="Enter New Pin">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="117.0" prefHeight="19.0" prefWidth="197.0" text="Pin Confirm">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <PasswordField fx:id="pinField" layoutX="24.0" layoutY="73.0" onKeyPressed="#enterCreate" promptText="Pin">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="pinConfirmField" layoutX="24.0" layoutY="138.0" onKeyPressed="#enterCreate" promptText="Pin confirm">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button layoutX="94.0" layoutY="316.0" mnemonicParsing="false" onAction="#create" style="-fx-background-color: gray;" text="Create" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <CheckBox fx:id="checkBox" layoutX="51.0" layoutY="200.0" mnemonicParsing="false" onAction="#select" prefHeight="30.0" prefWidth="150.0" text="MinusAccount">
         <font>
            <Font size="15.0" />
         </font>
      </CheckBox>
      <TextField fx:id="creditLimit" disable="true" layoutX="24.0" layoutY="247.0" onKeyPressed="#enterCreate" prefHeight="30.0" prefWidth="201.0" promptText="Credit Limit">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
