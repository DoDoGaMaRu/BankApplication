      <Label layoutX="100.0" layoutY="135.0" prefHeight="20.0" prefWidth="150.0" text="Withdraw amount :">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="depositAmount" layoutX="254.0" layoutY="134.0" prefHeight="22.0" prefWidth="132.0" promptText="Withdraw Amount" />
      <Label layoutX="394.0" layoutY="135.0" text="₩">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
