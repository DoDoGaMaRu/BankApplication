<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="310.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.AccountController">
   <children>
      <Label layoutX="100.0" layoutY="93.0" prefHeight="19.0" prefWidth="69.0" text="Balance :">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="balance" alignment="CENTER_RIGHT" layoutX="170.0" layoutY="93.0" prefHeight="20.0" prefWidth="119.0">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label layoutX="294.0" layoutY="93.0" text="₩">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="messageLabel" alignment="CENTER" layoutX="119.0" layoutY="266.0" prefHeight="30.0" prefWidth="283.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="withdrawButton" layoutX="302.0" layoutY="210.0" mnemonicParsing="false" onAction="#deposit" prefHeight="30.0" prefWidth="102.0" style="-fx-background-color: gray;" text="Withdraw" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button layoutX="18.0" layoutY="266.0" mnemonicParsing="false" onAction="#back" style="-fx-background-color: invisable;" text="&lt; Back">
