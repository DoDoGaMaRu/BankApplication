<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="310.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.CreateAccountController">
   <children>
      <Label layoutX="55.0" layoutY="40.0" prefHeight="24.0" prefWidth="139.0" text="Create Account">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="98.0" prefHeight="19.0" prefWidth="197.0" text="Enter New Pin">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="164.0" prefHeight="19.0" prefWidth="197.0" text="Pin Confirm">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <PasswordField fx:id="pinField" layoutX="24.0" layoutY="120.0" onKeyPressed="#enterCreate" promptText="Pin">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="pinConfirmField" layoutX="24.0" layoutY="185.0" onKeyPressed="#enterCreate" promptText="Pin confirm">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button layoutX="93.0" layoutY="252.0" mnemonicParsing="false" onAction="#create" style="-fx-background-color: gray;" text="Create" textFill="WHITE">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
